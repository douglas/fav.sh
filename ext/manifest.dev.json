{
  "manifest_version": 2,
  "key": "",
  "name": "Fav Bookmark Manager",
  "short_name": "Fav",
  "version": "0.2.0",
  "description": "An alternative bookmark manager that sync's with Github Gist",
  "icons": {
    "16": "icons/16.png",
    "48": "icons/48.png",
    "128": "icons/128.png"
  },
  "browser_action": {
    "default_icon": {
      "16": "icons/16.png",
      "24": "icons/24.png",
      "32": "icons/32.png"
    },
    "default_title": "Fav",
    "default_popup": "index.html",
    "content_security_policy": "script-src 'self' 'sha256-5As4+3YpY62+l38PsxCEkjB1R4YtyktBtRScTJ3fyLU=' object-src 'self'"
  },
  "background": {
    "scripts": ["background.js"],
    "persistence": false
  },
  "permissions": [
    "tabs",
    "storage",
    "contextMenus",
    "identity",
    "https://*.github.com/*",
    "https://*.fav.sh/*"
  ]
}
