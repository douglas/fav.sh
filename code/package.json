{
  "name": "fav.sh",
  "version": "2.0.0",
  "description": "Fav Bookmark Manager",
  "main": "index.js",
  "scripts": {
    "firefox": "run-p bundle start:firefox",
    "chrome": "run-p bundle start:chrome",
    "bundle": "NODE_ENV=development parcel ./src/manifest.json ./src/entry/settings.html",
    "build:firefox": "yarn clean && NODE_ENV=production parcel build ./src/manifest.json --no-source-maps && web-ext build --source-dir ./dist",
    "start:chrome": "web-ext run -t chromium --source-dir ./dist",
    "start:firefox": "web-ext run -t firefox-desktop --source-dir ./dist",
    "start:settings": "parcel ./src/entry/settings.html",
    "start:web": "parcel ./src/entry/web.html -p 9000",
    "format": "prettier --write './src/js/**/*.{ts,tsx}'",
    "clean": "rm -rf ./dist ./.cache ./web-ext-artifacts",
    "ci": "tsc && yarn run format"
  },
  "author": "Sunny Golovine",
  "license": "MIT",
  "dependencies": {
    "@material-ui/core": "^4.9.3",
    "file-saver": "^2.0.2",
    "lodash": "^4.17.15",
    "react": "^16.12.0",
    "react-dom": "^16.12.0",
    "react-redux": "^7.1.3",
    "react-router-dom": "^5.1.2",
    "react-sidebar": "^3.0.2",
    "redux": "^4.0.5",
    "redux-devtools": "^3.5.0",
    "redux-devtools-dock-monitor": "^1.1.3",
    "redux-devtools-log-monitor": "^1.4.0",
    "redux-persist": "^6.0.0",
    "styled-components": "^5.0.1",
    "uuid": "^3.4.0"
  },
  "devDependencies": {
    "@types/react-sidebar": "^3.0.0",
    "@types/uuid": "^3.4.7",
    "@babel/core": "^7.8.6",
    "@babel/plugin-transform-runtime": "^7.8.3",
    "@babel/preset-react": "^7.8.3",
    "@material-ui/types": "^5.0.0",
    "@types/file-saver": "^2.0.1",
    "@types/firefox-webext-browser": "^70.0.1",
    "@types/lodash": "^4.14.149",
    "@types/react": "^16.9.19",
    "@types/react-redux": "^7.1.7",
    "@types/react-router-dom": "^5.1.3",
    "@types/redux": "^3.6.0",
    "@types/redux-devtools": "^3.0.47",
    "@types/redux-devtools-dock-monitor": "^1.1.33",
    "@types/redux-devtools-log-monitor": "^1.0.34",
    "@types/styled-components": "^4.4.3",
    "babel-plugin-module-resolver": "^4.0.0",
    "npm-run-all": "^4.1.5",
    "parcel": "^1.12.4",
    "parcel-plugin-web-extension": "^1.6.1",
    "prettier": "^1.19.1",
    "typescript": "^3.7.5",
    "web-ext": "^4.1.0"
  }
}
