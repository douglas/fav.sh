{
  "name": "fav.sh",
  "version": "2.0.0",
  "description": "Fav Bookmark Manager",
  "main": "index.js",
  "scripts": {
    "firefox": "run-p bundle start:firefox",
    "chrome": "run-p bundle start:chrome",
    "bundle": "parcel ./src/manifest.json",
    "start:chrome": "web-ext run -t chromium --source-dir ./dist",
    "start:firefox": "web-ext run -t firefox-desktop --source-dir ./dist",
    "format": "prettier --write './src/js/**/*.{ts,tsx}'",
    "clean": "rm -rf ./dist ./.cache",
    "ci": "tsc && format"
  },
  "author": "Sunny Golovine",
  "license": "MIT",
  "dependencies": {
    "js": "^0.1.0",
    "react": "^16.12.0",
    "react-dom": "^16.12.0",
    "react-redux": "^7.1.3",
    "redux": "^4.0.5",
    "redux-devtools": "^3.5.0",
    "redux-devtools-dock-monitor": "^1.1.3",
    "redux-devtools-log-monitor": "^1.4.0",
    "redux-persist": "^6.0.0",
    "redux-persist-webextension-storage": "^1.0.2",
    "styled-components": "^5.0.1"
  },
  "devDependencies": {
    "@babel/core": "^7.8.4",
    "@babel/preset-react": "^7.8.3",
    "@types/react": "^16.9.19",
    "@types/react-redux": "^7.1.7",
    "@types/redux": "^3.6.0",
    "@types/redux-devtools": "^3.0.47",
    "@types/redux-devtools-dock-monitor": "^1.1.33",
    "@types/redux-devtools-log-monitor": "^1.0.34",
    "@types/styled-components": "^4.4.3",
    "babel-plugin-module-resolver": "^4.0.0",
    "npm-run-all": "^4.1.5",
    "parcel": "^1.12.4",
    "parcel-plugin-web-extension": "^1.6.1",
    "prettier": "^1.19.1",
    "typescript": "^3.7.5",
    "web-ext": "^4.1.0",
    "webpack": "^4.41.6",
    "webpack-cli": "^3.3.11"
  }
}
