{
  "name": "fav.sh",
  "version": "2.0.0",
  "description": "Fav Bookmark Manager",
  "main": "index.js",
  "scripts": {
    "start:ext": "parcel ./src/manifest.json",
    "start:web": "parcel ./src/entry/web.html",
    "start:firefox": "web-ext run --source-dir ./dist",
    "format": "prettier --write '**/*.{ts, tsx, js, jsx, md, json}'",
    "clean": "rm -rf ./dist ./.cache"
  },
  "author": "Sunny Golovine",
  "license": "MIT",
  "dependencies": {
    "react": "^16.12.0",
    "react-dom": "^16.12.0",
    "styled-components": "^5.0.1"
  },
  "devDependencies": {
    "@types/react": "^16.9.19",
    "@types/styled-components": "^4.4.3",
    "parcel": "^1.12.4",
    "parcel-plugin-web-extension": "^1.6.1",
    "prettier": "^1.19.1",
    "typescript": "^3.7.5",
    "web-ext": "^4.1.0",
    "webpack": "^4.41.6",
    "webpack-cli": "^3.3.11"
  }
}
